<h1>look all the empty spaces you could have to your car.</h1>

<div class="container">
  <div class="row">
    <% @parkings.each do |parking| %>
      <div class="col-xs-12 col-md-6 col-lg-4">
        <%= link_to parking_path(parking), class: 'parking-image' do %>
          <div class="parking-card">
            <%= parking.description %>
            <%= parking.address %>
            <%= parking.city %>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
  <div
    id="map"
    style="width: 100%;
    height: 600px;"
    data-markers="<%= @markers.to_json %>"
  ></div>
</div>

