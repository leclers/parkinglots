<h1>look all the empty spaces you could have to your car.</h1>

<div class="container">
  <div class="row">
    <% @parkings.each do |parking| %>
      <div class="col-xs-12 col-md-6 col-lg-4">
        <div class="parking-card">
        <%= parking.description %>
        <%= parking.address %>
        </div>
      </div>
    <% end %>
  </div>
</div>

