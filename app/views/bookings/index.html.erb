<h3> All Your Bookings</h3>

<div class="container">

  <div class="row">
    <% @requested_bookings.each do |booking| %>
          <div class="booking-card">
            <%= booking.start_time %>
            <%= booking.finish_time %>
            <%= booking.status %>
            <%= booking.parking_id %>
            <%= booking.user_id %>
            <%= booking.id %>

          </div>
    <% end %>
  </div>
</div>
